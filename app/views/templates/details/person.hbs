<div class="pane-people">
<div class="pagination"><ul>
{{#each person}}
    <li><a href="#">{{link_title}}</a></li>
{{/each}}
</ul></div>

{{#each person}}
  <div class="paginate-person" style="margin-bottom: 4px;">
  <img class="pull-left" src="{{avatar}}" style="margin-right: 8px;" width="50" height="50"/>
    <strong><a href="{{permalink}}">{{display_name}}</a></strong>:
    {{about}}
    <ul>
      <li><strong>Homepage: </strong><a href="{{website}}">{{website}}</a></li>
      <li><strong>Github: </strong>{{github}}</li>
      <li><strong>Twitter: </strong>{{twitter}}</li>
      <li><strong>Last Active: </strong>{{last_active}}</li>
    </ul>
  </div>
{{/each}}
</div>
